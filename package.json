{
  "name": "@blackbox-vision/website",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "lint": "eslint src/**/*",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@blackbox-vision/react-particles": "^1.2.0",
    "@blackbox-vision/ui-components": "^1.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.49",
    "@rehooks/input-value": "^1.0.0",
    "aos": "^2.3.4",
    "clsx": "^1.1.0",
    "dotenv": "^8.2.0",
    "final-form": "^4.19.1",
    "gatsby": "^4.25.7",
    "gatsby-image": "^2.3.3",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-facebook-pixel": "^1.0.3",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-google-gtag": "^2.0.1",
    "gatsby-plugin-hotjar": "^1.0.1",
    "gatsby-plugin-manifest": "^2.3.4",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-nprogress": "^2.2.2",
    "gatsby-plugin-offline": "^3.1.3",
    "gatsby-plugin-react-helmet": "^3.2.3",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sitemap": "^2.3.3",
    "gatsby-plugin-typography": "^2.4.2",
    "gatsby-source-cloudinary": "^0.1.13",
    "gatsby-source-filesystem": "^2.2.3",
    "i18next": "^19.4.2",
    "i18next-browser-languagedetector": "^4.1.0",
    "lodash.get": "^4.4.2",
    "react": "^16.13.1",
    "react-cookie-banner": "^4.1.0",
    "react-dom": "^16.13.1",
    "react-final-form": "^6.4.0",
    "react-headroom": "^3.0.0",
    "react-helmet": "^6.0.0",
    "react-i18next": "^11.3.4",
    "react-lazyload": "^2.6.7",
    "react-typography": "^0.16.19",
    "smooth-scroll": "^16.1.3",
    "typeface-ubuntu": "0.0.65",
    "typography": "^0.16.19",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "babel-loader": "^8.1.0",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,ts,tsx}": [
      "eslint --fix --ext .ts,.tsx,.js",
      "git add"
    ]
  }
}
